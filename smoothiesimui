function autoSellF()
spawn(function()
while getgenv().autoS == true do
game:GetService("ReplicatedStorage").Events.Gameplay.SellToHotAirBalloon:FireServer()
game:GetService("ReplicatedStorage").Events.Gameplay.MakeBalloonFly:FireServer()
wait(7)
end
end)
end
function autosellVF()
spawn(function()
while getgenv().autoVS == true do
game:GetService("ReplicatedStorage").Events.Gameplay.SellToVIPHotAirBalloon:FireServer()
game:GetService("ReplicatedStorage").Events.Gameplay.AddCratesToVIPBalloon:FireServer()
wait()
end
end)
end
