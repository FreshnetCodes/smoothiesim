b:Toggle("Send To Blender",function(bool)
getgenv().autoSTB = bool
if bool then
autoSTBF()
end
end)
b:Toggle("Collect From Blender",function(bool)
getgenv().autoCFB = bool
if bool then
autoCFBF()
end
end)
b:Toggle("Auto Sell",function(bool)
getgenv().autoS = bool
if bool then
autoSellF()
end
end)
b:Toggle("Auto Sell to VIP Balloon",function(bool)
getgenv().autoS = bool
if bool then
autosellVF() 
end
end)
c:Button("Smoothie Shop",function()
hum.CFrame = CFrame.new(40.3425, 2.20934, -277.465)
end)
c:Button("BackPack Shop",function()
hum.CFrame = CFrame.new(29.2104, 2.20908, -222.429)
end)
c:Button("Vip Balloon",function()
hum.CFrame = CFrame.new(-16.0274, 2.10962, -158.502)
end)
c:Button("Plot",function()
local plotPOS = localplot.Parts.EnterPart.Position
hum.CFrame = CFrame.new(plotPOS)
end)
c:Button("Destroy Water",function()
water:destroy()
end)
