    prompt.createPrompt("How to place trees over limit", "Execute Infinite Yeild, Type 'backtrack 10', Then auto-click place tree, You should have lots of trees in the same spot, you can type 'backtrack 0' to get normal ping", "Close", true, function(close)
        print("Button Clicked")
        close()
  end)
end)
